<textarea
    class="form-control wysiwyg"
    name="{{ element.getName }}"
    id="{{ element.getName }}"
    {% if element.getDisabled %}disabled{% endif %}
    >{{ element.getValue }}</textarea>
    
<script>
    $(function() {
        $("#{{ element.getName }}").summernote({{ element.getOptions|json_encode()|raw }});
    });
</script>

