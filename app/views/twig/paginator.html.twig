<div class="text-center">
    <ul class="pagination">
        <li class="disabled">
            <a href="">
                Results <strong>{{ paginator.getTotalResults }}</strong>
            </a>
        </li>

        {% if paginator.getCurrentPage > 1 %}
        <li>
            <a href="{{ url(routeId, {
                    page: 1
                }|merge(routeParams)) }}">First</a>
        </li>
        {% else %}
        <li class="disabled">
            <a href="#">First</a>
        </li>
        {% endif %}
        
        <!-- Start Go back //-->

        {% if paginator.getCurrentPage > 1 %}
        <li>
            <a href="{{ url(routeId, {
                    page: paginator.getCurrentPage - 1
                }|merge(routeParams)) }}">Prev</a>
        </li>
        {% else %}
        <li class="disabled">
            <a href="#">Prev</a>
        </li>
        {% endif %}

        <!-- End Go back //-->

        <!-- Start Pages list //-->

        {% for i in range(1, paginator.getTotalPages) %}
            <li{% if i == paginator.getCurrentPage %} class="active"{% endif %}>
                <a href="{{ url(routeId, {
                    page: i
                }|merge(routeParams)) }}">
                    {{ i }}
                </a>
            </li>
        {% endfor %}

        <!-- End Pages list //-->

        <!-- Start Go next //-->

        {% if paginator.getCurrentPage < paginator.getTotalPages %}
        <li>
            <a href="{{ url(routeId, {
                page: paginator.getCurrentPage+1
            }|merge(routeParams)) }}">Next</a>
        </li>
        {% else %}
        <li class="disabled">
            <a href="#">Next</a>
        </li>
        {% endif %}

        <!-- End Go next //-->
        
        {% if paginator.getCurrentPage < paginator.getTotalPages %}
        <li>
            <a href="{{ url(routeId, {
                page: paginator.getTotalPages
            }|merge(routeParams)) }}">Last</a>
        </li>
        {% else %}
        <li class="disabled">
            <a href="#">Last</a>
        </li>
        {% endif %}

        <li class="disabled">
            <a href="">
                Page
                <strong>{{ paginator.getCurrentPage }}</strong>
                of
                <strong>{{ paginator.getTotalPages }}</strong>
            </a>
        </li>
    </ul>
</div>